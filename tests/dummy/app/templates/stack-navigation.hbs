{{#ons-page}}
  {{#ons-toolbar}}
    <div class="left">
      {{#unless hideBackButton}}
        {{ons-back-button events=(hash click=(action "goBack"))}}
      {{/unless}}
    </div>

    <div class="center">
      Navigation Stack Demo
    </div>

    <div class="right">
      {{#ons-toolbar-button events=(hash click=(action (mut menu.collapsed) false))}}
        {{ons-icon icon="md-menu"}}
      {{/ons-toolbar-button}}
    </div>
  {{/ons-toolbar}}

  {{liquid-outlet}}
{{/ons-page}}
