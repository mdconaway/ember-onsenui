{{#ons-page}}
  {{#ons-toolbar}}
    <div class="left">
      {{#ons-toolbar-button click=(action "updateCursor" -1)}}
        {{ons-icon icon="md-chevron-left"}}
      {{/ons-toolbar-button}}
    </div>

    <div class="center">
      Carousel Example
    </div>

    <div class="right">
      {{#ons-toolbar-button click=(action "updateCursor" 1)}}
        {{ons-icon icon="md-chevron-right"}}
      {{/ons-toolbar-button}}

      {{#ons-toolbar-button click=(action (mut menu.collapsed) false)}}
        {{ons-icon icon="md-menu"}}
      {{/ons-toolbar-button}}
    </div>
  {{/ons-toolbar}}

  <div class="content">
    {{#liquid-bind model
        class="carousel-animation"
        as |currentModel|}}
      {{component currentModel.componentName update=(action "updateCursor")}}
    {{/liquid-bind}}
  </div>
{{/ons-page}}
